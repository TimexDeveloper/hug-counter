{
  "version": 2,
  "builds": [
    {
      "src": "api/hug.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/api/hug",
      "dest": "/api/hug.py"
    },
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/templates/index.html"
    }
  ]
}